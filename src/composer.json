{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.0",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.9",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "TeamTeaTime\\Forum\\": "/var/www/packages/laravel-forum/src/",
            "TeamTeaTime\\Forum\\Database\\Factories\\": "/var/www/packages/laravel-forum/database/factories/",
            "TeamTeaTime\\Forum\\Database\\Seeds\\": "/var/www/packages/laravel-forum/database/seeds/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
